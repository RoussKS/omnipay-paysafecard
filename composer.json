{
    "name": "worldstream-labs/omnipay-paysafecard",
    "type": "library",
    "description": "Paysafecard driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "paysafecard",
        "omnipay",
        "pay",
        "payment"
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Paysafecard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Paysafecard\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^7.1.0 || ^8.0.0",
        "omnipay/common": "^3.0.1",
        "ext-json": "*",
        "symfony/http-client": "^7.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.1 || ^4.0",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Brouwers",
            "email": "mb@worldstream.nl"
        },
        {
            "name": "Rory Scholman",
            "email": "rory.scholman@worldstream.nl"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
